{
  "name": "@zoo/ui",
  "description": "Unified UI components for all Zoo Network properties.",
  "version": "2.0.0",
  "author": "Zoo Protocol",
  "license": "MIT",
  "source": "src/index.ts",
  "exports": "./dist/index.module.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "nft",
    "zoo",
    "zookit",
    "protocol",
    "sdk"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && npm run build:css",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css --minify",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "version": "yarn version",
    "version:package": "sh ../../scripts/package-version.sh",
    "version:update": "yarn version ${0}; PACKAGE_VERSION=$(yarn version:package); git add -A; git commit -m \"ðŸŽ‰ Release ui package v$PACKAGE_VERSION\"; git tag v${PACKAGE_VERSION}-UI; git push --tags;",
    "version:prerelease": "yarn version prerelease; RC_VERSION=$(yarn version:package); git add -A; git commit -m \"âœ¨ Prerelease ui package v$RC_VERSION\"; git tag v${RC_VERSION}-UI; git push --tags;",
    "changelog": "node ../../scripts/generate-changelog.js package=ui"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@react-hookz/web": "^19.2.0",
    "@reservoir0x/reservoir-sdk": "1.1.8",
    "@stitches/react": "1.3.1-1",
    "dayjs": "^1.11.4",
    "flatpickr": "^4.6.13",
    "framer-motion": "^6.4.2",
    "react-flatpickr": "^3.10.13",
    "swr": "2.0.1"
  },
  "peerDependencies": {
    "react": "^18.0 || ^19.0",
    "react-dom": "^18.0 || ^19.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zoo-labs/zoo-kit"
  }
}
