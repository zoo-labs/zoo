    "/points/proposals": {
      "get": {
        "tags": ["points"],
        "summary": "Get proposal count for address",
        "description": "Get the number of proposals that an address has submitted",
        "security": [],
        "parameters": [
          {
            "name": "address",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Wallet address"
          },
          {
            "name": "passed",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean"
            },
            "description": "Whether to filter for passed proposals"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "address": {
                      "type": "string"
                    },
                    "proposalCount": {
                      "type": "integer"
                    },
                    "status": {
                      "type": "string",
                      "enum": ["success", "failed"]
                    }
                  },
                  "required": ["address", "proposalCount", "status"]
                }
              }
            }
          }
        }
      }
    },
