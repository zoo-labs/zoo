{
  "name": "@zoolabs/zoo",
  "version": "5.0.0",
  "author": "Zoo Labs Foundation <hi@zoolabs.io> (https://zoolabs.io)",
  "license": "MIT",
  "keywords": [
    "ai",
    "ethereum",
    "nft",
    "zoo",
    "zoolabs",
    "marketplace",
    "metaverse"
  ],
  "private": true,
  "scripts": {
    "build": "yarn build:all",
    "build:all": "yarn build:zdk && yarn build:app",
    "build:app": "yarn workspace @zoolabs/app build",
    "build:foundation": "yarn workspace @zoolabs/foundation build",
    "build:lab": "yarn workspace @zoolabs/lab build",
    "build:packages": "parcel build --no-cache sdk ui && yarn build:fix-type-defs",
    "build:fix-type-defs": "node ./scripts/fix-type-defs-imports",
    "build:zdk": "yarn workspace @zoolabs/zdk build",
    "build:core": "yarn workspace @zoolabs/core build",
    "clean": "rm -rf packages/app/.next && rm -rf contracts/artifacts && rm -rf contracts/cache && rm -rf contracts/types",
    "typecheck": "tsc --skipLibCheck --noEmit -p .",
    "reset": "yarn clean && rm -rf node_modules .yarn/cache .parcel-cache",
    "start": "yarn workspace @zoolabs/app start",
    "start:lab": "yarn workspace @zoolabs/lab start",
    "start:network": "yarn workspace @zoolabs/network dev",
    "start:fund": "yarn workspace @zoolabs/fund dev",
    "account": "yarn workspace @zoolabs/contracts account",
    "accounts": "yarn workspace @zoolabs/contracts accounts",
    "balance": "yarn workspace @zoolabs/contracts balance",
    "chain": "yarn workspace @zoolabs/contracts chain",
    "chain2": "yarn workspace @zoolabs/contracts chain2",
    "compile": "yarn workspace @zoolabs/contracts compile",
    "deploy": "yarn workspace @zoolabs/contracts deploy",
    "deploy:game": "yarn workspace @zoolabs/contracts deploy:game",
    "deploy:mainnet": "yarn workspace @zoolabs/contracts deploy:mainnet",
    "deploy:testnet": "yarn workspace @zoolabs/contracts deploy:testnet",
    "dev": "yarn workspace @zoolabs/app dev",
    "dev:core": "yarn workspace @zoolabs/core dev",
    "dev:foundation": "yarn workspace @zoolabs/foundation dev",
    "configure": "yarn workspace @zoolabs/contracts configure",
    "configure:testnet": "yarn workspace @zoolabs/contracts configure:testnet",
    "configure:mainnet": "yarn workspace @zoolabs/contracts configure:mainnet",
    "export": "yarn workspace @zoolabs/contracts export",
    "flatten": "cd packages/contracts && npx hardhat flatten",
    "fork": "yarn workspace @zoolabs/contracts fork",
    "fundedwallet": "cd packages/contracts && npx hardhat fundedwallet",
    "generate": "yarn workspace @zoolabs/contracts generate",
    "matrix": "yarn workspace @zoolabs/contracts matrix",
    "mineContractAddress": "cd packages/contracts && npx contracts mineContractAddress",
    "node": "yarn workspace @zoolabs/contracts chain",
    "send": "yarn workspace @zoolabs/contracts send",
    "test": "yarn workspace @zoolabs/contracts test",
    "wallet": "cd packages/contracts && npx contracts wallet",
    "drop": "yarn workspace @zoolabs/moralis drop",
    "functions": "yarn workspace @zoolabs/moralis functions",
    "logs": "yarn workspace @zoolabs/moralis logs",
    "plugins": "yarn workspace @zoolabs/moralis plugins",
    "proxy": "yarn workspace @zoolabs/moralis proxy",
    "watch": "yarn workspace @zoolabs/moralis watch",
    "lab": "yarn workspace @zoolabs/lab start",
    "subgraph:codegen": "yarn workspace @zoolabs/subgraph codegen",
    "subgraph:prepare-local": "yarn workspace @zoolabs/subgraph prepare:local",
    "subgraph:prepare-testnet": "yarn workspace @zoolabs/subgraph prepare:testnet",
    "subgraph:prepare-mainnet": "yarn workspace @zoolabs/subgraph prepare:mainnet",
    "subgraph:build": "yarn workspace @zoolabs/subgraph build",
    "subgraph:create-local": "yarn workspace @zoolabs/subgraph create:local",
    "subgraph:deploy-local": "yarn workspace @zoolabs/subgraph deploy:local",
    "subgraph:deploy-testnet": "yarn workspace @zoolabs/subgraph deploy:testnet",
    "subgraph:deploy-mainnet": "yarn workspace @zoolabs/subgraph deploy:mainnet",
    "subgraph:start": "yarn workspace @zoolabs/subgraph start",
    "graphnode:start": "yarn workspace @zoolabs/graphnode start",
    "format": "yarn workspace @zoolabs/app format && yarn workspace @zoolabs/contracts format && yarn workspace @zoolabs/moralis format"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@parcel/babel-plugin-transform-runtime": "^2.8.3",
    "@parcel/babel-preset-env": "^2.8.3",
    "@parcel/config-default": "^2.8.3",
    "@parcel/optimizer-data-url": "2.8.3",
    "@parcel/packager-ts": "2.8.3",
    "@parcel/transformer-inline-string": "2.8.3",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@types/node": "^18.0",
    "@types/react": "^18.0",
    "@types/react-dom": "^18.0",
    "buffer": "^6.0.3",
    "ethers": "^5.7.2",
    "gitlog": "^4.0.4",
    "parcel": "^2.8.3",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "replace-in-files": "^3.0.0"
  },
  "engines": {
    "node": ">=18",
    "yarn": ">=1"
  },
  "workspaces": {
    "packages": [
      "app",
      "contracts",
      "core",
      "foundation",
      "fund",
      "lab",
      "network",
      "sdk",
      "ui",
      "zdk"
    ],
    "nohoist": [
      "**/@openzeppelin/contracts-upgradeable/**",
      "**/@openzeppelin/contracts",
      "**/@zoolabs/hardhat",
      "**/@zoolabs/solidity"
    ]
  }
}
